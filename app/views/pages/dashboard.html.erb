<%= render 'shared/navbar' %>
<div class="container mt-5">
  <h1>My Listings</h1>
  <div class="px-4 py-3">
    <% @offers.each do |offer| %>
      <%= render partial: 'shared/card-dashboard-listings', locals: {offer: offer} %>
    <% end %>
  </div>
  <h1>My bookings</h1>
  <div class="mt-3 px-4 py-3">
    <% @offers.each do |offer| %>
      <%= render partial: 'shared/card-dashboard-rentals', locals: {offer: offer} %>
    <% end %>
  </div>
</div>
